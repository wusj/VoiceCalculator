<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
xmlns:android="http://schemas.android.com/apk/res/android">
<PreferenceCategory android:title="@string/feedback">
<CheckBoxPreference
android:key="voice_on"
android:defaultValue="true"
android:title="@string/voice"
android:summary="@string/voicesummary"
/>
<CheckBoxPreference
android:key="haptic_on"
android:defaultValue="true"
android:title="@string/haptic"
android:summary="@string/hapticsummary"
/>
</PreferenceCategory>
<PreferenceCategory android:title="@string/voice_pkg">
        <ListPreference
                android:key="voice_pkg"
                android:defaultValue="default"
                android:dependency="voice_on"
                android:dialogTitle="@string/select_voice" 
        />
</PreferenceCategory>
</PreferenceScreen>
